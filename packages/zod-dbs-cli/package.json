{
  "name": "zod-dbs-cli",
  "version": "1.0.21",
  "description": "Opinionated Zod schema generator for databases",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "bin": {
    "zod-dbs": "index.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc --project tsconfig.build.json",
    "test": "pnpm run test:unit && pnpm run test:integration",
    "test:unit": "vitest --project unit run",
    "test:integration": "vitest --project integration run",
    "test:types": "tsc --noEmit"
  },
  "author": "Magnus Solv√•g",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msunbay/zod-dbs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "mustache": "^4.2.0",
    "ora": "^8.2.0",
    "zod-dbs": "workspace:*",
    "zod-dbs-core": "workspace:*"
  }
}
