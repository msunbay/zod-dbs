{
  "name": "zod-dbs",
  "version": "2.0.2",
  "description": "Opinionated Zod schema generator for databases",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "zod",
    "zod-dbs",
    "typescript",
    "database",
    "schema",
    "schema-generator",
    "type-safe",
    "validation",
    "sql",
    "nosql",
    "postgresql",
    "mysql",
    "mongodb",
    "mssql",
    "oracle",
    "sqlite",
    "snowflake",
    "codegen",
    "database-schema"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc --project tsconfig.build.json",
    "test": "pnpm run test:unit",
    "test:unit": "vitest --project unit run",
    "test:integration": "vitest --project integration run",
    "test:types": "tsc --noEmit"
  },
  "author": "msunbay",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msunbay/zod-dbs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "templates"
  ],
  "dependencies": {
    "mustache": "^4.2.0",
    "pluralize": "^8.0.0",
    "zod-dbs-core": "workspace:*"
  }
}
